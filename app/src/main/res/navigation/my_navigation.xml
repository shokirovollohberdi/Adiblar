<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/my_navigation"
    app:startDestination="@id/homeFragment">

    <fragment
        android:id="@+id/homeFragment"
        android:name="uz.shokirov.adiblarhaqida.HomeFragment"
        android:label="fragment_home"
        tools:layout="@layout/fragment_home" >
        <action
            android:id="@+id/action_homeFragment_to_settingsFragment2"
            app:destination="@id/settingsFragment" />
        <action
            android:id="@+id/action_homeFragment_to_savedFragment"
            app:destination="@id/savedFragment" />
        <action
            android:id="@+id/action_homeFragment_to_lifeGrandsFragment"
            app:destination="@id/lifeGrandsFragment" />
        <action
            android:id="@+id/action_homeFragment_to_addAdibFragment"
            app:destination="@id/addAdibFragment" />
    </fragment>
    <fragment
        android:id="@+id/lifeGrandsFragment"
        android:name="uz.shokirov.adiblarhaqida.LifeGrandsFragment"
        android:label="fragment_life_grands"
        tools:layout="@layout/fragment_life_grands" >
        <action
            android:id="@+id/action_lifeGrandsFragment_to_mumtozFragment"
            app:destination="@id/mumtozFragment" />
        <action
            android:id="@+id/action_lifeGrandsFragment_to_uzbekFragment"
            app:destination="@id/uzbekFragment" />
        <action
            android:id="@+id/action_lifeGrandsFragment_to_jahonFragment"
            app:destination="@id/jahonFragment" />
        <action
            android:id="@+id/action_lifeGrandsFragment_to_searchFragment"
            app:destination="@id/searchFragment" />
    </fragment>
    <fragment
        android:id="@+id/savedFragment"
        android:name="uz.shokirov.adiblarhaqida.SavedFragment"
        android:label="fragment_saved"
        tools:layout="@layout/fragment_saved" >
        <action
            android:id="@+id/action_savedFragment_to_searchFragment"
            app:destination="@id/searchFragment" />
        <action
            android:id="@+id/action_savedFragment_to_infoFragment"
            app:destination="@id/infoFragment" />
    </fragment>
    <fragment
        android:id="@+id/settingsFragment"
        android:name="uz.shokirov.adiblarhaqida.SettingsFragment"
        android:label="fragment_settings"
        tools:layout="@layout/fragment_settings" >
        <action
            android:id="@+id/action_settingsFragment_to_savedFragment"
            app:destination="@id/savedFragment" />
    </fragment>
    <fragment
        android:id="@+id/addAdibFragment"
        android:name="uz.shokirov.adiblarhaqida.AddAdibFragment"
        android:label="fragment_add_adib"
        tools:layout="@layout/fragment_add_adib" />
    <fragment
        android:id="@+id/mumtozFragment"
        android:name="uz.shokirov.adiblarhaqida.MumtozFragment"
        android:label="fragment_mumtoz"
        tools:layout="@layout/fragment_mumtoz" >
        <action
            android:id="@+id/action_mumtozFragment_to_infoFragment"
            app:destination="@id/infoFragment" />
    </fragment>
    <fragment
        android:id="@+id/uzbekFragment"
        android:name="uz.shokirov.adiblarhaqida.UzbekFragment"
        android:label="fragment_uzbek"
        tools:layout="@layout/fragment_uzbek" >
        <action
            android:id="@+id/action_uzbekFragment_to_infoFragment"
            app:destination="@id/infoFragment" />
    </fragment>
    <fragment
        android:id="@+id/jahonFragment"
        android:name="uz.shokirov.adiblarhaqida.JahonFragment"
        android:label="fragment_jahon"
        tools:layout="@layout/fragment_jahon" >
        <action
            android:id="@+id/action_jahonFragment_to_infoFragment"
            app:destination="@id/infoFragment" />
    </fragment>
    <fragment
        android:id="@+id/searchFragment"
        android:name="uz.shokirov.adiblarhaqida.SearchFragment"
        android:label="fragment_search"
        tools:layout="@layout/fragment_search" >
        <action
            android:id="@+id/action_searchFragment_to_infoFragment"
            app:destination="@id/infoFragment" />
    </fragment>
    <fragment
        android:id="@+id/infoFragment"
        android:name="uz.shokirov.adiblarhaqida.InfoFragment"
        android:label="fragment_info"
        tools:layout="@layout/fragment_info" />
</navigation>